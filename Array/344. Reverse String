class Solution {
    public void reverseString(char[] s) {
        //two pointers variables
        int left = 0;
        int right = s.length-1;
        while(left<=right){
            //normal swapping
            char temp = s[left];
            s[left] = s[right];
            s[right] = temp;
            left++;
            right--;
        }
    }
}
